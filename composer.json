{
    "name": "arrocy/laravel-sortable",
    "description": "Sortable behavior package for Laravel",
    "keywords": [
        "laravel",
        "sort",
        "sortable",
        "sorting",
        "model",
        "view",
        "blade",
        "column"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arrocy",
            "email": "info@arrocy.com",
            "homepage": "https://arrocy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "orchestra/testbench": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Arrocy\\Sortable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrocy\\Sortable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arrocy\\Sortable\\Provider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
